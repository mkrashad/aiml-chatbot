	<!-- Music -->
	<category>
		<pattern>
			MUSIC
		</pattern>
		<template>
		 I can show you random music videos 
		 <random>
		     <li>
		      <![CDATA[<video width="500" controls src="http://dl128.y2mate.com/?"></video>]]>
		       Twenty One Pilots - Ride
		     </li>
		       <li>
		         <![CDATA[<video width="500" controls src="http://dl104.y2mate.com/?%3D%3D"></video>]]>
