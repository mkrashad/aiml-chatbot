	<!-- Goodbyes -->
	<category>
		<pattern>
			THANK YOU
		</pattern>
		<template>
			<random>
				<li>
					You very welcome
				</li>
				<li>
					No problem
				</li>
				<li>
					Ask anytime
				</li>
			</random>
		</template>
	</category>
	<category>
		<pattern>
			BYE
		</pattern>
		<template>
			<random>
				<li>
					Bye!
				</li>
				<li>
				<image>https://media.giphy.com/media/KctrWMQ7u9D2du0YmD/giphy.gif</image>
				</li>
				<li>
					Have a nice day!
