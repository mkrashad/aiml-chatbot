<?xml version="1.0" encoding="UTF-8"?>
<aiml>
    
	<!-- Error message -->
	<category>
		<pattern>
			*
		</pattern>
		<template>
			Sorry, could not understand you please write your question again.
		</template>
	</category>
	
	<!-- Greetings message -->
	<category>
		<pattern>
			HELLO
		</pattern>
		<template>
			Hello, I am Mr.Robot and my goal is to motivate you Firstly I would like to know your age.
		</template>
	</category>
	<category>
		<pattern>
			HELLO *
		</pattern>
		<template>
			Hello, I am Mr.Robot and my goal is to motivate you. How old are you?
		</template>
	</category>
	<category>
		<pattern>
			HI
		</pattern>
		<template>
			<random>
				<li>
					Hello, I am Mr.Robot and my goal is to motivate you Firstly I would like to know your age.
				</li>
				<li>
					Hi there, I am Mr.Robot and my goal is to motivate you. How old are you?
				</li>
				<li>
					Good to see you, I am Mr.Robot and my goal is to motivate you. Please tell me your age.
				</li>
			</random>
		</template>
	</category>
	<category>
		<pattern>
			HI *
		</pattern>
		<template>
			Hello, I am Mr.Robot and my goal is to motivate you Firstly I would like to know your age.
		</template>
	</category>
	
	<!-- Years -->
	<category>
		<pattern>
			 * years
		</pattern>
		<template>
			Excellent, now I would like to know your name and lastname.
		</template>
	</category>
	
	<!-- Name -->
	<category>
		<pattern>
		     * *
		</pattern>
		<template>
			Nice to meet you
			<set name="firstname"> <star index="1" /> </set>
			<set name="lastname">
				<star index="2" />
			</set>
			.
			I can recommend you top great movies or top songs. What do you like movies or music?
			<reply>
				<text>
					Movies
				</text>
				<postback>
					Movies
				</postback>
			</reply>
			<reply>
				<text>
					Music
				</text>
				<postback>
					Music
				</postback>
			</reply>
		</template>
	</category>
	
	<!-- Checking the name -->
	<category>
		<pattern>
			WHAT IS MY FIRSTNAME
		</pattern>
		<template>
			Your firstname is
			<get name="firstname">
				.
			</get>
		</template>
	</category>
	<category>
		<pattern>
			WHAT IS MY LASTNAME
		</pattern>
		<template>
			Your lastname is
			<get name="lastname">
				.
			</get>
		</template>
	</category>
	<category>
		<pattern>
			WHAT IS MY FULLNAME
		</pattern>
		<template>
			Your name is
			<get name="firstname">
			</get>
			<get name="lastname">
				.
			</get>
		</template>
	</category>
	
